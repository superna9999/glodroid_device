CONFIG_FASTBOOT_FLASH_MMC_DEV=2
#  KEEP ALPHABETICALLY SORTED
# CONFIG_DISTRO_DEFAULTS is not set
CONFIG_BOOTCOMMAND="if test ${boot_source} = \"usb\"; then fastboot 0; fi; setenv scriptaddr 0x53000000 ; setenv mmc_bootdev 2 ; load mmc ${mmc_bootdev}:1 ${scriptaddr} boot.scr && source ${scriptaddr} ; setenv mmc_bootdev 1 ; load mmc ${mmc_bootdev}:1 ${scriptaddr} boot.scr && source ${scriptaddr}"
CONFIG_FASTBOOT_BUF_ADDR=0x6000000
CONFIG_FASTBOOT_BUF_SIZE=0x1000000
